{
  "extends": ["airbnb"],
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-one-expression-per-line": "off",
    "no-prototype-builtins": "off",
    "react/destructuring-assignment": "off"
  }
}
